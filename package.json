{
  "name": "eshop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lost": "^8.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-scripts": "0.9.x"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-assets": "^5.0.0",
    "postcss-cli": "^6.1.2",
    "postcss-css-variables": "^0.13.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "precss": "^4.0.0",
    "react-router-dom": "^5.0.1"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.scss -o src/index.css",
    "watch:css": "postcss src/styles/index.scss -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build": "npm-run-all build:css build-js",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
